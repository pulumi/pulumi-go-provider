{
  "name": "go-components",
  "version": "0.1.0",
  "namespace": "examples",
  "meta": {
    "moduleFormat": "(.*)"
  },
  "language": {
    "csharp": {
      "respectSchemaVersion": true
    },
    "go": {
      "generateResourceContainerTypes": true,
      "respectSchemaVersion": true
    },
    "nodejs": {
      "respectSchemaVersion": true
    },
    "python": {
      "respectSchemaVersion": true
    }
  },
  "config": {},
  "provider": {
    "type": "object"
  },
  "resources": {
    "go-components:index:RandomComponent": {
      "properties": {
        "hardcodedOutput": {
          "type": "string"
        },
        "length": {
          "type": "integer"
        },
        "password": {
          "$ref": "/random/v4.8.1/schema.json#/resources/random:index%2FrandomPassword:RandomPassword"
        }
      },
      "type": "object",
      "required": [
        "hardcodedOutput",
        "length",
        "password"
      ],
      "inputProperties": {
        "length": {
          "type": "integer"
        }
      },
      "requiredInputs": [
        "length"
      ],
      "isComponent": true
    },
    "go-components:nested:NestedRandomComponent": {
      "properties": {
        "hardcodedOutput": {
          "type": "string"
        },
        "length": {
          "type": "integer"
        },
        "password": {
          "$ref": "/random/v4.8.1/schema.json#/resources/random:index%2FrandomPassword:RandomPassword"
        }
      },
      "type": "object",
      "required": [
        "hardcodedOutput",
        "length",
        "password"
      ],
      "inputProperties": {
        "length": {
          "type": "integer"
        }
      },
      "requiredInputs": [
        "length"
      ],
      "isComponent": true
    }
  }
}
