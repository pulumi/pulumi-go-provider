{
  "name": "dna-store",
  "version": "0.1.0",
  "meta": {
    "moduleFormat": "(.*)"
  },
  "config": {},
  "types": {
    "dna-store:index:Metadata": {
      "properties": {
        "sampleType": {
          "$ref": "#/types/dna-store:index:SampleType",
          "description": "sample type of the dna"
        },
        "tags": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          },
          "description": "optional tags associated with the dna sample"
        }
      },
      "type": "object",
      "required": [
        "sampleType"
      ]
    },
    "dna-store:index:Molecule": {
      "type": "integer",
      "enum": [
        {
          "description": "adenine",
          "value": 0
        },
        {
          "description": "cytosine",
          "value": 1
        },
        {
          "description": "thymine",
          "value": 2
        },
        {
          "description": "guanine",
          "value": 3
        }
      ]
    },
    "dna-store:index:SampleType": {
      "type": "string",
      "enum": [
        {
          "value": "human"
        },
        {
          "value": "dog"
        },
        {
          "value": "cat"
        },
        {
          "value": "other"
        }
      ]
    }
  },
  "provider": {
    "type": "object"
  },
  "resources": {
    "dna-store:index:DNAStore": {
      "properties": {
        "data": {
          "type": "array",
          "items": {
            "$ref": "#/types/dna-store:index:Molecule"
          },
          "description": "molecule data"
        },
        "filedir": {
          "type": "string"
        },
        "metadata": {
          "$ref": "#/types/dna-store:index:Metadata",
          "description": "stores information related to a particular dna"
        }
      },
      "type": "object",
      "required": [
        "data",
        "filedir",
        "metadata"
      ],
      "inputProperties": {
        "data": {
          "type": "array",
          "items": {
            "$ref": "#/types/dna-store:index:Molecule"
          },
          "description": "molecule data"
        },
        "filedir": {
          "type": "string"
        },
        "metadata": {
          "$ref": "#/types/dna-store:index:Metadata",
          "description": "stores information related to a particular dna"
        }
      },
      "requiredInputs": [
        "data",
        "filedir",
        "metadata"
      ]
    }
  }
}
